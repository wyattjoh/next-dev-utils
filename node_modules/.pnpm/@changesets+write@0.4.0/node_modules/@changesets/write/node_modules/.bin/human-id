#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/human-id@4.1.1/node_modules/human-id/dist/node_modules:/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/human-id@4.1.1/node_modules/human-id/node_modules:/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/human-id@4.1.1/node_modules:/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/human-id@4.1.1/node_modules/human-id/dist/node_modules:/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/human-id@4.1.1/node_modules/human-id/node_modules:/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/human-id@4.1.1/node_modules:/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../human-id@4.1.1/node_modules/human-id/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../human-id@4.1.1/node_modules/human-id/dist/cli.js" "$@"
fi
