#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/syncpack@12.3.0/node_modules/syncpack/dist/bin-update/node_modules:/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/syncpack@12.3.0/node_modules/syncpack/dist/node_modules:/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/syncpack@12.3.0/node_modules/syncpack/node_modules:/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/syncpack@12.3.0/node_modules:/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/syncpack@12.3.0/node_modules/syncpack/dist/bin-update/node_modules:/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/syncpack@12.3.0/node_modules/syncpack/dist/node_modules:/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/syncpack@12.3.0/node_modules/syncpack/node_modules:/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/syncpack@12.3.0/node_modules:/home/runner/work/next-dev-utils/next-dev-utils/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../syncpack/dist/bin-update/index.js" "$@"
else
  exec node  "$basedir/../syncpack/dist/bin-update/index.js" "$@"
fi
