{
  "name": "@wyattjoh/next-dev-utils",
  "version": "2.2.0",
  "exports": {
    ".": "./src/cli.ts",
    "./lib": "./src/lib/index.ts"
  },
  "$schema": "https://github.com/denoland/deno/blob/main/cli/schemas/config-file.v1.json",
  "license": "MIT",
  "publish": {
    "include": ["src/**/*.ts", "README.md", "LICENSE"]
  },
  "tasks": {
    "build": "deno compile --allow-all --output=./nu src/cli.ts",
    "hooks:install": "deno run --allow-read=deno.json,.git/hooks/ --allow-write=.git/hooks/ jsr:@hongminhee/deno-task-hooks",
    "hooks:pre-commit": "deno check && deno fmt --check && deno lint",
    "hooks:pre-push": "deno publish --dry-run --allow-dirty"
  },
  "imports": {
    "@bradenmacdonald/s3-lite-client": "jsr:@bradenmacdonald/s3-lite-client@^0.9.2",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.5",
    "@valibot/valibot": "jsr:@valibot/valibot@^1.1.0",
    "clipboardy": "npm:clipboardy@^4.0.0",
    "execa": "npm:execa@^9.6.0",
    "find-cache-directory": "npm:find-cache-directory@^6.0.0",
    "@cliffy/command": "jsr:@cliffy/command@^1.0.0-rc.8",
    "@cliffy/prompt": "jsr:@cliffy/prompt@^1.0.0-rc.8",
    "strip-indent": "npm:strip-indent@^4.0.0",
    "ora": "npm:ora@^8.2.0"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "exactOptionalPropertyTypes": true
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 80,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": false,
    "proseWrap": "preserve"
  },
  "lint": {
    "rules": {
      "tags": ["recommended"]
    }
  }
}
